{
  "name": "lib-testing",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test:unit": "node --experimental-specifier-resolution=node --loader ts-node/esm node_modules/mocha/lib/cli/cli.js unit-tests/**/*.spec.ts",
    "generate": "node --loader ts-node/esm --experimental-specifier-resolution=node generateDemoFiles.ts",
    "test:browser": "cd browser-tests && npm test",
    "test": "npm run test:unit && npm run test:browser"
  },
  "dependencies": {
    "assert-js": "^1.0.0",
    "better-assert": "^1.0.2",
    "cheerio": "^1.0.0-rc.10",
    "mocha": "^9.1.3"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.3",
    "ts-node": "^10.4.0"
  }
}
