{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2021",
    "sourceMap": true,
    "composite": true,
    "rootDir": "./",
    "lib": ["ES2020", "ESNext.Array"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "paths": {
      "wink-eng-lite-web-model": ["./wink-eng-lite-web-model.js"],
      "wink-nlp": ["./wink-nlp.js"]
